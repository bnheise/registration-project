{
	"name": "profile-web",
	"version": "1.0.0",
	"description": "Profile Web",
	"devDependencies": {
		"@babel/cli": "^7.7.5",
		"@babel/core": "^7.7.5",
		"@babel/preset-env": "^7.7.6",
		"@babel/preset-react": "^7.7.4",
		"@babel/preset-typescript": "^7.16.0",
		"@types/node": "^16.11.11",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/react-router-dom": "^5.3.2",
		"copy-webpack-plugin": "^10.0.0",
		"liferay-npm-build-support": "^2.28.0",
		"liferay-npm-bundler": "^2.15.1",
		"liferay-types": "file:../../../../liferay-types",
		"typescript": "^4.5.2",
		"webpack": "^5.64.4",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.6.0"
	},
	"dependencies": {
		"react": "16.8.6",
		"react-dom": "16.8.6",
		"react-router": "^5.2.1",
		"react-router-dom": "^5.3.0"
	},
	"scripts": {
		"build": "npm run check-types && npx babel src -d build src --extensions '.ts,.tsx' && npm run copy-assets && liferay-npm-bundler",
		"check-types": "tsc",
		"copy-assets": "lnbs-copy-assets",
		"translate": "lnbs-translate",
		"deploy": "npm run build && lnbs-deploy",
		"start": "lnbs-start"
	},
	"portlet": {
		"com.liferay.portlet.display-category": "amf custom",
		"com.liferay.portlet.header-portlet-css": "/css/styles.css",
		"com.liferay.portlet.instanceable": false,
		"javax.portlet.name": "profileweb",
		"javax.portlet.security-role-ref": "power-user,user",
		"javax.portlet.resource-bundle": "content.Language"
	},
	"main": "index.js"
}

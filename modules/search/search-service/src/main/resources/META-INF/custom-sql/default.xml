<?xml version="1.0" encoding="UTF-8"?>
<custom-sql>
    <sql id="findByZip">
        <![CDATA[
            SELECT *
            FROM useraddress
            WHERE zip = ?
            LIMIT ?
            OFFSET ?;
        ]]>
    </sql>
    <sql id="getCountByZip">
        <![CDATA[
            SELECT
                COUNT(*)
            FROM useraddress
            WHERE zip = ?;
        ]]>
    </sql>
</custom-sql>